  <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="packagedefs">
      <xs:complexType>
        <xs:sequence>
          <xs:element name="defaultdateformat" type="xs:string"/>
          <xs:element name="defaultdateonlyformat" type="xs:string"/>
          <xs:element ref="packagetype" maxOccurs="unbounded"/>
          <xs:element ref="fieldgroup" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:element name="packagetype">
      <xs:complexType>
        <xs:sequence>
          <xs:element ref="name"/>
          <xs:element ref="label"/>
          <xs:element ref="description"/>
          <xs:element ref="imageconfiguration" minOccurs="0"/>
          <xs:element ref="objects" minOccurs="0"/>
          <xs:element ref="displaygroup" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:element name="fieldgroup">
      <xs:complexType>
        <xs:sequence>
          <xs:element ref="field" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="name" type="xs:string"/>
      </xs:complexType>
    </xs:element>
    <xs:element name="field">
      <xs:complexType>
        <xs:sequence>
          <xs:element ref="name"/>
          <xs:element ref="label"/>
          <xs:element ref="description"/>
          <xs:element ref="inherits"/>
          <xs:element ref="default"/>
          <xs:element ref="texttag"/>
          <xs:element ref="datatype"/>
          <xs:element ref="constraint" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:element name="imageconfiguration">
      <xs:complexType>
        <xs:sequence>
          <xs:element ref="imagerelation" minOccurs="0"/>
          <xs:element ref="imagesettings" minOccurs="0"/>
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:element name="imagerelation">
      <xs:complexType>
        <xs:sequence>
          <xs:element ref="datatype"/>
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:element name="imagesettings">
      <xs:complexType>
        <xs:sequence>
          <xs:element ref="cropsetting" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element ref="gallerysetting" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:element name="cropsetting">
      <xs:complexType>
        <xs:attribute name="id" type="xs:string"/>
        <xs:attribute name="validate" type="xs:string"/>
      </xs:complexType>
    </xs:element>
    <xs:element name="gallerysetting">
      <xs:complexType>
        <xs:attribute name="id" type="xs:string"/>
        <xs:attribute name="validate" type="xs:string"/>
      </xs:complexType>
    </xs:element>
    <xs:element name="objects">
      <xs:complexType>
        <xs:sequence>
          <xs:element ref="object" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:element name="object">
      <xs:complexType>
        <xs:sequence>
          <xs:element ref="name"/>
          <xs:element ref="contextual-use"/>
        </xs:sequence>
        <xs:attribute name="type" type="xs:string"/>
        <xs:attribute name="validate" type="xs:string"/>
      </xs:complexType>
    </xs:element>
    <xs:element name="displaygroup">
      <xs:complexType>
        <xs:sequence>
          <xs:element ref="name"/>
          <xs:element ref="label"/>
          <xs:element ref="description"/>
          <xs:element ref="fieldgroup" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:element name="datatype">
      <xs:complexType>
        <xs:sequence>
          <xs:element ref="option" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="type" type="xs:string"/>
      </xs:complexType>
    </xs:element>
    <xs:element name="option">
      <xs:complexType>
        <xs:attribute name="value" type="xs:string"/>
      </xs:complexType>
    </xs:element>
    <xs:element name="constraint">
      <xs:complexType>
        <xs:attribute name="type" type="xs:string"/>
        <xs:attribute name="value" type="xs:string"/>
      </xs:complexType>
    </xs:element>
    <xs:element name="description" type="xs:string"/>
    <xs:element name="inherits" type="xs:string"/>
    <xs:element name="name" type="xs:string"/>
    <xs:element name="label" type="xs:string"/>
    <xs:element name="default" type="xs:string"/>
    <xs:element name="texttag" type="xs:string"/>
    <xs:element name="contextual-use" type="xs:string"/>
  </xs:schema>