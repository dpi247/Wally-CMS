<?php

/**
 * @file
 * Bulk export of panels_layouts objects generated by Bulk export module.
 */

/**
 * Implementation of hook_default_panels_renderer_pipeline().
 */
function cpsprint_default_panels_renderer_pipeline() {
  $pipelines = array();

  $pipeline = new stdClass;
  $pipeline->disabled = FALSE; /* Edit this to true to make a default pipeline disabled initially */
  $pipeline->api_version = 1;
  $pipeline->name = 'cpsprint';
  $pipeline->admin_title = t('CPS Print');
  $pipeline->admin_description = t('To print cps.');
  $pipeline->weight = 0;
  //This is add to know that this a print pipeline
  $pipeline->display_type = 'print';
  $pipeline->settings = array(
    
    'renderers' => array(
      0 => array(
        'access' => array(),
        'renderer' => 'cpsprint',
        'options' => array(),
      ),
    ),
  );
  $pipelines[$pipeline->name] = $pipeline;

  return $pipelines;
}
